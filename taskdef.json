{
  "executionRoleArn": "arn:aws:iam::869935096522:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 3000,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "environment": [
        {
          "name": "DB_PORT",
          "value": "5432"
        },
        {
          "name": "DB_USER",
          "value": "postgres"
        },
        {
          "name": "SECRET_TOKEN",
          "value": "secretoken"
        }
      ],
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "name": "container-blacklist"
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "512",
  "memory": "1024",
  "family": "blacklist-task"
}
